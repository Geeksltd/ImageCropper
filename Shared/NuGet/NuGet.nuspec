<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.ImageCropper</id>
    <version>1.0.9</version>
    <title>ImageCropper for Zebble (Xamarin)</title>
    <projectUrl>https://github.com/Geeksltd/Zebble.ImageCropper</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.ImageCropper/master/Shared/NuGet/Icon.png</iconUrl>
    <copyright>Copyright © 2020 Geeks Ltd - All rights reserved.</copyright>
    <description>ImageCropper is a Zebble implementation for cropping image on Android and IOS platform.</description>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="Zebble.Media" version="1.1.105" />
        <dependency id="Xamarin.Android.Support.Exif" version="28.0.0.1" />
        <dependency id="Xamarin.Android.Support.v7.AppCompat" version="28.0.0.1" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Zebble.Media" version="1.1.105" />
      </group>
    </dependencies>
    <references>
      <group targetFramework="MonoAndroid10">
        <reference file="Binding_AndroidImageCropper.dll" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <reference file="TOCropViewController.dll" />
      </group>
    </references>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.ImageCropper.UWP.*" target="lib\UAP10\" />
    <file src="..\..\lib\Zebble.ImageCropper.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\TOCropViewController.dll" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.ImageCropper.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Binding_AndroidImageCropper.dll" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.ImageCropper.UWP.*" target="ref\uap10.0" />
  </files>
</package>